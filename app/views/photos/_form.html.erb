<%= form_for [@event, photo], html: {multipart: true} do |f| %>
  <%= render partial: 'partials/form_errors', locals: {model: photo} %>

  <div class="form-group">
    <%= f.label :photo %>
    <%= f.file_field :photo, id: 'photo' %>
  </div>

  <div class="actions">
    <%= f.submit 'Загрузить', id: 'submitButton' ,class: 'btn btn-primary' %>
  </div>
<% end %>
